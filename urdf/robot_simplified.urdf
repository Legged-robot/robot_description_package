<!-- This file contains the description of the FORE robot. -->
<robot name="robot"
    xmlns:xacro="http://www.ros.org/wiki/xacro">  
    <!-- Material for the visual primitives -->  
    <material name="robot_material">    
        <color rgba="0.7 0.7 0.7 1.0"/>  
    </material>  
    <!--                            <<<<<<<<<<<<    Base link    >>>>>>>>>>>>                        --> 
    <link name="base">    
        <visual>      
            <origin rpy="0 0 0" xyz="0 0 0"/>      
            <geometry>        
                <box size="0.4 0.2 0.05"/>      
            </geometry>        
            <material name="robot_material"/>    
        </visual>    
        <collision>      
            <origin rpy="0 0 0" xyz="0 0 0"/>      
            <geometry>        
                <box size="0.4 0.2 0.05"/>      
            </geometry>           
        </collision>            
    </link>  
    <!-- Fixed joint to add dummy inertia link -->  
    <joint name="base_to_base_inertia" type="fixed">    
        <parent link="base"/>    
        <child link="base_inertia"/>    
        <origin rpy="0 0 0" xyz="0 0 0"/>  
    </joint>  
    <!-- Dummy inertia link, because KDL cannot have inertia on the base link -->  
    <link name="base_inertia">    
        <inertial>      
            <origin rpy="0 0 0" xyz="-0.001960558279 -0.001413217745 0.050207125344"/>      
            <mass value="16.793507758"/>      
            <inertia ixx="0.217391101503" ixy="-0.00132873239126" ixz="-0.00228200226173" iyy="0.639432546734" iyz="-0.00138078263145" izz="0.62414077654"/>    
        </inertial>  
    </link>  
    <!--                            <<<<<<<<<<<<    THIGH    >>>>>>>>>>>>                        -->
    <link name="LF_THIGH">    
    <!-- r is around y, p is around x, y is around z Pi/2 is 1.57079632679  -->
        <visual>      
            <origin rpy="0.0 0 0" xyz="0.0 0.0 -0.07"/>        
            <geometry>        
                <box size="0.03 0.01 0.14"/>      
            </geometry>     
            <material name="robot_material"/>    
        </visual>    
        <visual>      
            <origin rpy="1.57079632679 0 0" xyz="0 0 0"/>     
            <geometry>        
                <cylinder length="0.01" radius="0.02"/>      
            </geometry>       
            <material name="robot_material"/>    
        </visual> 
        <visual>      
            <origin rpy="1.57079632679 0 0" xyz="0 0 -0.14"/>     
            <geometry>        
                <cylinder length="0.01" radius="0.02"/>      
            </geometry>       
            <material name="robot_material"/>    
        </visual>      
        <!-- Collision geometry -->
        <collision>      
            <origin rpy="0.0 0 0" xyz="0.0 0.0 -0.07"/>        
            <geometry>        
                <box size="0.03 0.01 0.14"/>      
            </geometry>     
            <material name="robot_material"/>    
        </collision>    
        <collision>      
            <origin rpy="1.57079632679 0 0" xyz="0 0 0"/>     
            <geometry>        
                <cylinder length="0.01" radius="0.02"/>      
            </geometry>       
            <material name="robot_material"/>    
        </collision> 
        <collision>      
            <origin rpy="1.57079632679 0 0" xyz="0 0 -0.14"/>     
            <geometry>        
                <cylinder length="0.01" radius="0.02"/>      
            </geometry>       
            <material name="robot_material"/>    
        </collision> 

        <inertial>      
            <origin rpy="0 0 0" xyz="0.064516258147 -0.003787101702 -0.000152184388"/>      
            <mass value="1.42462064"/>      
            <inertia ixx="0.00243023349564" ixy="-1.53023971e-05" ixz="-2.1819095354e-05" iyy="0.00230257239103" iyz="2.6473021273e-05" izz="0.0019806759227"/>    
        </inertial>  
    </link>  

    <!-- Hip joint -->  
    <joint name="LF_JHIP" type="revolute">    
        <parent link="base"/>    
        <child link="LF_THIGH"/>    
        <origin xyz="0.19 0.11 0.0"/>    
        <axis xyz="0 1 0"/>    
        <limit command_effort="40" current="10" effort="80" gear_velocity="10" lower="-9.42" upper="9.42" velocity="15"/>    
        <dynamics damping="0.0" friction="0.0"/>  
    </joint>  
    <!--                            <<<<<<<<<<<<    CALF    >>>>>>>>>>>>                        -->
    <link name="LF_CALF">    
        <visual>      
            <origin rpy="0.0 0 0" xyz="0.0 0.0 -0.065"/>        
            <geometry>        
                <box size="0.03 0.01 0.13"/>      
            </geometry>     
            <material name="robot_material"/>    
        </visual>   
        <visual>      
            <origin rpy="1.57079632679 0 0" xyz="0 0 0"/>     
            <geometry>        
                <cylinder length="0.01" radius="0.02"/>      
            </geometry>       
            <material name="robot_material"/>    
        </visual>   
        <visual>      
            <origin rpy="1.57079632679 0 0" xyz="0 0 -0.13"/>     
            <geometry>        
                <cylinder length="0.01" radius="0.02"/>      
            </geometry>       
            <material name="robot_material"/>    
        </visual>  
        <!-- Collision geometry  -->
        <collision>      
            <origin rpy="0.0 0 0" xyz="0.0 0.0 -0.065"/>        
            <geometry>        
                <box size="0.03 0.01 0.13"/>      
            </geometry>     
            <material name="robot_material"/>    
        </collision>   
        <collision>      
            <origin rpy="1.57079632679 0 0" xyz="0 0 0"/>     
            <geometry>        
                <cylinder length="0.01" radius="0.02"/>      
            </geometry>       
            <material name="robot_material"/>    
        </collision>   
        <collision>      
            <origin rpy="1.57079632679 0 0" xyz="0 0 -0.13"/>     
            <geometry>        
                <cylinder length="0.01" radius="0.02"/>      
            </geometry>       
            <material name="robot_material"/>    
        </collision>     
        <inertial>      
            <origin rpy="0 0 0" xyz="-0.003897968082 0.054226618537 -0.214583373795"/>      
            <mass value="1.634976467"/>      
            <inertia ixx="0.0120367944369" ixy="6.762065206e-05" ixz="0.000287806340448" iyy="0.0120643637939" iyz="-0.00140610131218" izz="0.00249422574881"/>    
        </inertial>  
    </link>  

    <!-- Knee joint -->  
    <joint name="LF_JKNEE" type="revolute">    
        <parent link="LF_THIGH"/>    
        <child link="LF_CALF"/>    
        <origin xyz="0.0                    0.0105                    -0.14"/>    
        <axis xyz="0 1 0"/>    
        <limit command_effort="40" current="10" effort="80" gear_velocity="10" lower="-9.42" upper="9.42" velocity="15.0"/>    
        <dynamics damping="0.0" friction="0.0"/>  
    </joint>  
    <!--                            <<<<<<<<<<<<    FOOT    >>>>>>>>>>>>                        -->
    <link name="LF_FOOT">    
        <visual>      
            <origin rpy="0.0 0 0" xyz="0.0 0.0 -0.045"/>        
            <geometry>        
                <box size="0.02 0.01 0.09"/>      
            </geometry>      
            <material name="robot_material"/>    
        </visual>  
        <visual>      
            <origin rpy="1.57079632679 0 0" xyz="0 0 0"/>     
            <geometry>        
                <cylinder length="0.01" radius="0.02"/>      
            </geometry>       
            <material name="robot_material"/>    
        </visual>   
        <!--    Collision geometry  -->
        <collision>      
            <origin rpy="0.0 0 0" xyz="0.0 0.0 -0.045"/>        
            <geometry>        
                <box size="0.02 0.01 0.09"/>      
            </geometry>      
            <material name="robot_material"/>    
        </collision>  
        <collision>      
            <origin rpy="1.57079632679 0 0" xyz="0 0 0"/>     
            <geometry>        
                <cylinder length="0.01" radius="0.02"/>      
            </geometry>       
            <material name="robot_material"/>    
        </collision>   

        <inertial>      
            <origin rpy="0 0 0" xyz="0.030816858139 -0.004617229294 0.000893125713"/>      
            <mass value="0.207204302"/>      
            <inertia ixx="0.0002104880248" ixy="-5.6750980345e-05" ixz="1.0127699391e-05" iyy="0.000676270210023" iyz="-8.22869024e-07" izz="0.000545032674924"/>    
        </inertial>  
    </link>  

    <!-- Anckle joint -->  
    <joint name="LF_JANCKLE" type="revolute">    
        <parent link="LF_CALF"/>    
        <child link="LF_FOOT"/>    
        <origin xyz="0.0                    -0.0105                    -0.13"/>    
        <axis xyz="0 1 0"/>    
        <limit command_effort="40" current="10" effort="80" gear_velocity="10" lower="-9.42" upper="9.42" velocity="15.0"/>    
        <dynamics damping="0.0" friction="0.0"/>  
    </joint>  

    <!--                            <<<<<<<<<<<<    FOOTPOINT    >>>>>>>>>>>>                        -->
    <link name="LF_FOOTPOINT">  
        <visual>      
            <origin xyz="0 0 0"/>      
            <geometry>        
                <sphere radius="0.005"/>      
            </geometry>    
        </visual>    
        <collision>      
            <origin xyz="0 0 0"/>      
            <geometry>        
                <sphere radius="0.005"/>      
            </geometry>    
        </collision>  
    </link>  

    <!-- Adapter to Foot joint -->  
    <joint name="LF_JFOOTPOINT" type="fixed">    
        <parent link="LF_FOOT"/>    
        <child link="LF_FOOTPOINT"/>    
        <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 -0.09"/>  
    </joint>  

    <!-- Gazebo customization -->  
    <gazebo reference="LF_FOOTPOINT">    
        <kp>1000000.0</kp>    
        <kd>100.0</kd>    
        <mu1>0.8</mu1>    
        <mu2>0.8</mu2>  
    </gazebo>  
</robot>

