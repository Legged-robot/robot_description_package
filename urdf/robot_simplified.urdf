<!-- This file contains the description of the FORE robot. -->
<robot name="robot"
    xmlns:xacro="http://www.ros.org/wiki/xacro">  
    <!-- Xacro:Properties -->
    <!-- [kg * m^2] -->
    <!-- [A] -->
    
    <!-- Material for the visual primitives -->  
    <material name="robot_material">    
        <color rgba="0.7 0.7 0.7 1.0"/>  
    </material>  
    <!--                            <<<<<<<<<<<<    Base link    >>>>>>>>>>>>                        --> 
    <link name="base"> 
        <visual>      
            <origin xyz="0 0 0"/>      
            <geometry>        
                <sphere radius="0.02"/>      
            </geometry>    
        </visual>              
    </link>  

    <!--                            <<<<<<<<<<<<    BASE_INERTIAL    >>>>>>>>>>>>                        -->
    <!-- Dummy inertia link, because KDL cannot have inertia on the base link -->  
    <link name="base_inertial">  
        <visual>      
            <origin rpy="0 0 0" xyz="0 0 0"/>      
            <geometry>        
                <box size="0.4 0.2 0.05"/>      
            </geometry>        
            <material name="robot_material"/>    
        </visual>    
        <collision>      
            <origin rpy="0 0 0" xyz="0 0 0"/>      
            <geometry>        
                <box size="0.4 0.2 0.05"/>      
            </geometry>           
        </collision>   
        <inertial>      
            <origin rpy="0 0 0" xyz="0. 0. 0."/>      
            <mass value="0.2565404"/>      
            <inertia ixx="6.338e-04" ixy="5.4452284e-05" ixz="5.826e-17" iyy="1.612e-04" iyz="-3.595e-16" izz="7.086e-04"/>    
        </inertial>  
    </link>  

    <!-- Fixed joint to add dummy inertia link -->  
    <joint name="base_to_base_inertial" type="fixed">    
        <parent link="base"/>    
        <child link="base_inertial"/>    
        <origin rpy="0 0 0" xyz="0 0 0"/>  
        <limit lower="-10" upper="30" effort="1000.0" velocity="10"/> 
        <axis xyz="0 0 1"/>   
    </joint>  

    <!--                            <<<<<<<<<<<<    THIGH    >>>>>>>>>>>>                        -->
    <link name="LF_THIGH">    
    <!-- r is around y, p is around x, y is around z Pi/2 is 1.57079632679  -->
        <visual>      
            <origin rpy="0.0 0 0" xyz="0.0 0.0 -0.07"/>        
            <geometry>        
                <box size="0.03 0.01 0.14"/>      
            </geometry>     
            <material name="robot_material"/>    
        </visual>    
        <visual>      
            <origin rpy="1.57079632679 0 0" xyz="0 0 0"/>     
            <geometry>        
                <cylinder length="0.01" radius="0.02"/>      
            </geometry>       
            <material name="robot_material"/>    
        </visual> 
        <visual>      
            <origin rpy="1.57079632679 0 0" xyz="0 0 -0.14"/>     
            <geometry>        
                <cylinder length="0.01" radius="0.02"/>      
            </geometry>       
            <material name="robot_material"/>    
        </visual>      
        <!-- Collision geometry -->
        <collision>      
            <origin rpy="0.0 0 0" xyz="0.0 0.0 -0.07"/>        
            <geometry>        
                <box size="0.03 0.01 0.14"/>      
            </geometry>     
            <material name="robot_material"/>    
        </collision>    
        <collision>      
            <origin rpy="1.57079632679 0 0" xyz="0 0 0"/>     
            <geometry>        
                <cylinder length="0.01" radius="0.02"/>      
            </geometry>       
            <material name="robot_material"/>    
        </collision> 
        <collision>      
            <origin rpy="1.57079632679 0 0" xyz="0 0 -0.14"/>     
            <geometry>        
                <cylinder length="0.01" radius="0.02"/>      
            </geometry>       
            <material name="robot_material"/>    
        </collision> 

        <inertial>      
            <origin rpy="0 0 0" xyz="0.0 0.0 -0.07"/>      
            <mass value="0.497881"/>      
            <inertia ixx="1.468e-03" ixy="-4.233e-15" ixz="9.3992189e-05" iyy="1.610e-03" iyz="-2.316e-15" izz="4.965e-04"/>     
        </inertial>  
    </link>  

    <!-- Hip joint -->  
    <joint name="LF_JHIP" type="revolute">    
        <parent link="base_inertial"/>    
        <child link="LF_THIGH"/>    
        <origin xyz="0.19 0.11 0.0"/>    
        <axis xyz="0 1 0"/>    
        <limit command_effort="40" current="10" effort="80" gear_velocity="10" lower="-9.42" upper="9.42" velocity="15"/>    
        <dynamics damping="0.0" friction="0.0"/>  
    </joint>  
    <!--                            <<<<<<<<<<<<    CALF    >>>>>>>>>>>>                        -->
    <link name="LF_CALF">    
        <visual>      
            <origin rpy="0.0 0 0" xyz="0.0 0.0 -0.065"/>        
            <geometry>        
                <box size="0.03 0.01 0.13"/>      
            </geometry>     
            <material name="robot_material"/>    
        </visual>   
        <visual>      
            <origin rpy="1.57079632679 0 0" xyz="0 0 0"/>     
            <geometry>        
                <cylinder length="0.01" radius="0.02"/>      
            </geometry>       
            <material name="robot_material"/>    
        </visual>   
        <visual>      
            <origin rpy="1.57079632679 0 0" xyz="0 0 -0.13"/>     
            <geometry>        
                <cylinder length="0.01" radius="0.02"/>      
            </geometry>       
            <material name="robot_material"/>    
        </visual>  
        <!-- Collision geometry  -->
        <collision>      
            <origin rpy="0.0 0 0" xyz="0.0 0.0 -0.065"/>        
            <geometry>        
                <box size="0.03 0.01 0.13"/>      
            </geometry>     
            <material name="robot_material"/>    
        </collision>   
        <collision>      
            <origin rpy="1.57079632679 0 0" xyz="0 0 0"/>     
            <geometry>        
                <cylinder length="0.01" radius="0.02"/>      
            </geometry>       
            <material name="robot_material"/>    
        </collision>   
        <collision>      
            <origin rpy="1.57079632679 0 0" xyz="0 0 -0.13"/>     
            <geometry>        
                <cylinder length="0.01" radius="0.02"/>      
            </geometry>       
            <material name="robot_material"/>    
        </collision>     
        <inertial>      
            <origin rpy="0 0 0" xyz="0.0 0.0 -0.065"/>      
            <mass value="0.082921"/>      
            <inertia ixx="1.544e-04" ixy="1.595e-17" ixz="-3.1006794e-05" iyy="1.569e-04" iyz="6.006e-18" izz="1.6461495e-05"/>  
        </inertial>  
    </link>  

    <!-- Knee joint -->  
    <joint name="LF_JKNEE" type="revolute">    
        <parent link="LF_THIGH"/>    
        <child link="LF_CALF"/>    
        <origin xyz="0.0                    0.0105                    -0.14"/>    
        <axis xyz="0 1 0"/>    
        <limit command_effort="40" current="10" effort="80" gear_velocity="10" lower="-9.42" upper="9.42" velocity="15.0"/>    
        <dynamics damping="0.0" friction="0.0"/>  
    </joint>  
    <!--                            <<<<<<<<<<<<    FOOT    >>>>>>>>>>>>                        -->
    <link name="LF_FOOT">    
        <visual>      
            <origin rpy="0.0 0 0" xyz="0.0 0.0 -0.045"/>        
            <geometry>        
                <box size="0.02 0.01 0.09"/>      
            </geometry>      
            <material name="robot_material"/>    
        </visual>  
        <visual>      
            <origin rpy="1.57079632679 0 0" xyz="0 0 0"/>     
            <geometry>        
                <cylinder length="0.01" radius="0.02"/>      
            </geometry>       
            <material name="robot_material"/>    
        </visual>   
        <!--    Collision geometry  -->
        <collision>      
            <origin rpy="0.0 0 0" xyz="0.0 0.0 -0.045"/>        
            <geometry>        
                <box size="0.02 0.01 0.09"/>      
            </geometry>      
            <material name="robot_material"/>    
        </collision>  
        <collision>      
            <origin rpy="1.57079632679 0 0" xyz="0 0 0"/>     
            <geometry>        
                <cylinder length="0.01" radius="0.02"/>      
            </geometry>       
            <material name="robot_material"/>    
        </collision>   

        <inertial>      
            <origin rpy="0 0 0" xyz="0.0 0.0 -0.045"/>      
            <mass value="0.0023701"/>      
            <inertia ixx=" 2.7634609e-04" ixy="-2.819e-18" ixz="-2.0650e-07" iyy="2.6780627e-05" iyz="-7.092e-16" izz="1.148229e-06"/> 
        </inertial>  
    </link>  

    <!-- Anckle joint -->  
    <joint name="LF_JANCKLE" type="revolute">    
        <parent link="LF_CALF"/>    
        <child link="LF_FOOT"/>    
        <origin xyz="0.0                    -0.0105                    -0.13"/>    
        <axis xyz="0 1 0"/>    
        <limit command_effort="40" current="10" effort="80" gear_velocity="10" lower="-9.42" upper="9.42" velocity="15.0"/>    
        <dynamics damping="0.0" friction="0.0"/>  
    </joint>  

    <!--                            <<<<<<<<<<<<    FOOTPOINT    >>>>>>>>>>>>                        -->
    <link name="LF_FOOTPOINT">  
        <visual>      
            <origin xyz="0 0 0"/>      
            <geometry>        
                <sphere radius="0.005"/>      
            </geometry>    
        </visual>    
        <collision>      
            <origin xyz="0 0 0"/>      
            <geometry>        
                <sphere radius="0.005"/>      
            </geometry>    
        </collision>  
    </link>  

    <!-- Adapter to Foot joint -->  
    <joint name="LF_JFOOTPOINT" type="fixed">    
        <parent link="LF_FOOT"/>    
        <child link="LF_FOOTPOINT"/>    
        <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 -0.09"/>  
    </joint>  

    <!-- Gazebo customization -->  
    <gazebo reference="LF_FOOTPOINT">    
        <kp>1000000.0</kp>    
        <kd>100.0</kd>    
        <mu1>0.8</mu1>    
        <mu2>0.8</mu2>  
    </gazebo>  
</robot>

